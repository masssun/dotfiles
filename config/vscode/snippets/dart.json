      "void main() {",
      "  late FeatureUseCase usecase;",
      "",
      "  final delegate = MockDelegate();",
      "",
      "  setUp(() {",
      "    reset(delegate);",
      "",
      "    usecase = ProviderContainer(overrides: [",
      "    ]).read(featureUseCaseProvider(delegate));",
      "  });",
      "",
      "  group('group', () {",
      "    test('test', () async {",
      "      usecase;",
      "",
      "      verify(delegate);",
      "",
      "      verifyNoMoreInteractions(delegate);",
      "    });",
      "  });",
